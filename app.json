{
  "expo": {
    "name": "nursi-connection-app",
    "slug": "nursi-connection-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Logo_col.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Logo_col.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.pallav1503.nursiconnectionapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Logo_col.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/mainLogo.png",
          "imageWidth": 100,
          "resizeMode": "contain",
          "backgroundColor": "#1856d9"
        }
      ],
      [
        "expo-secure-store",
        {
          "configureAndroidBackup": true,
          "faceIDPermission": "Allow $(PRODUCT_NAME) to access your Face ID biometric data."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f44cf119-adec-4516-872c-a6de0ddbc530"
      }
    }
  }
}
