{
  "cli": {
    "version": ">= 3.16.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "dev",
      "android": {
        "buildType": "apk",
        "resourceClass": "medium"
      },
      "ios": {
        "simulator": true,
        "resourceClass": "medium"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "android": {
        "buildType": "apk",
        "resourceClass": "medium"
      },
      "ios": {
        "buildConfiguration": "Release",
        "resourceClass": "medium"
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "android": {
        "buildType": "apk",
        "resourceClass": "medium"
      },
      "ios": {
        "buildConfiguration": "Release",
        "resourceClass": "medium"
      }
    }
  },
  "submit": {
    "production": {
    }
  }
}
